#include <stdio.h>
int main()
{
	int arr[5] = {10, 20, 30, 40, 50};
	int arr1[5]= {50, 40, 30, 20, 10};
	int *p = arr;
	int *q = arr1;
	int t;
	for (int i = 0; i < 5; i++)
	{

		t=*(p+i);
		*(p+i)=*(q+i);
		*(q+i)=t;
		
	}
	for (int i = 0; i < 5; i++)
	{
	    printf("p[i] is %d\n",p[i]);
	
	}
	for (int i = 0; i < 5; i++)
	{
	    printf(" q[i] is %d\n",q[i]);
	
	}
}
