#include <stdio.h>
int swap(int *x, int *y)
{
    int t;
    t =*x;
    *x=*y;
    *y=t;
}
int main()
{
   int a,b;
   printf("enter value of a:");
   scanf("%d",&a);
   printf("enter value of b:");
   scanf("%d",&b);
   printf("before swapping of two numbers is a =%d,b=%d\n",a,b);
   swap(&a,&b);
    printf("after swapping of two numbers is a =%d,b=%d",a,b);
   return 0;
}
