#include<stdio.h>
void point(int *p, int *q, int n)
{
    
    for(int i=0;i<n;i++)  
  {
    q[i]=p[i];
    printf("%d ",q[i]);
  }
}
int main()
{
    int arr[5]={10, 20, 30, 40, 50};
    int arr1[5];
    int *p= arr;
    int *q= arr1;
    void (*fptr)(int *, int *, int)=point;
    fptr(p,q,5);
    return 0;
}
