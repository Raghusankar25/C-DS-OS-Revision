#include<stdio.h>
#define N 4
int main()
{
    char str[20];
    char temp;
    int len,i;
    printf("enter the str: ");
    scanf("%s",str);
    for(len=0;str[len]!='\0';len++);
    for(i=0;i<N/2;i++)
    {
    temp=str[len-(N-i)];
   str[len-(N-i)]=str[len-(i+1)];
   str[len-(i+1)]=temp;
    }
    printf("%s",str);
    return 0;
}
