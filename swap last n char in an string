#include<stdio.h>
#define R 4
int main()
{
    char s[50];
    char temp;
    int len,i;
    printf("enter the str: ");
    scanf("%s",s);
    for(len=0;s[len]!='\0';len++);
    for(i=0;i<len-2;i++)
    {
    temp=s[len-(R-i)];
   s[len-(R-i)]=s[len-(i+1)];
   s[len-(i+1)]=temp;
    }
    printf("%s",s);
    return 0;
}
